{
  "name": "slimgym",
  "version": "1.3.2",
  "description": "An indentation-based configuration format parser for the 24th and Â½ century!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./parse": {
      "types": "./dist/parse.d.ts",
      "import": "./dist/parse.js",
      "require": "./dist/parse.js"
    },
    "./slimgify": {
      "types": "./dist/slimgify.d.ts",
      "import": "./dist/slimgify.js",
      "require": "./dist/slimgify.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "publish:unversioned": "pnpm run build && pnpm publish --no-git-checks",
    "publish:patch": "pnpm version patch && pnpm publish --no-git-checks",
    "publish:minor": "pnpm version minor && pnpm publish --no-git-checks",
    "publish:major": "pnpm version major && pnpm publish --no-git-checks",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:stress": "vitest run slimgym.stress.test.ts"
  },
  "packageManager": "pnpm@10.0.0",
  "keywords": [
    "config",
    "parser",
    "configuration",
    "yaml",
    "json",
    "indentation",
    "slimgym"
  ],
  "author": "",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliranbenishai/slimgym.git"
  },
  "bugs": {
    "url": "https://github.com/eliranbenishai/slimgym/issues"
  },
  "homepage": "https://slimgym.dev",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^4.0.0",
    "eslint": "^9.0.0",
    "jiti": "^2.6.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
