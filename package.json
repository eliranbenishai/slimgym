{
  "name": "slimgym",
  "version": "1.1.2",
  "description": "JSON is too verbose and lacks modern features. YAML is too restrictive. SlimGym isn't.",
  "main": "dist/parser.js",
  "types": "dist/parser.d.ts",
  "exports": {
    ".": {
      "types": "./dist/parser.d.ts",
      "import": "./dist/parser.js",
      "require": "./dist/parser.js"
    }
  },
  "files": [
    "dist/parser.js",
    "dist/parser.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "publish": "pnpm run build && pnpm publish",
    "publish:patch": "pnpm version patch && pnpm run build && pnpm publish --no-git-checks",
    "publish:minor": "pnpm version minor && pnpm run build && pnpm publish --no-git-checks",
    "publish:major": "pnpm version major && pnpm run build && pnpm publish --no-git-checks",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "packageManager": "pnpm@10.0.0",
  "keywords": [
    "config",
    "parser",
    "configuration",
    "yaml",
    "json",
    "indentation",
    "slimgym"
  ],
  "author": "",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliranbenishai/slimgym.git"
  },
  "bugs": {
    "url": "https://github.com/eliranbenishai/slimgym/issues"
  },
  "homepage": "https://github.com/eliranbenishai/slimgym#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^24.10.1",
    "@vitest/ui": "^4.0.0",
    "eslint": "^9.0.0",
    "jiti": "^2.6.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
